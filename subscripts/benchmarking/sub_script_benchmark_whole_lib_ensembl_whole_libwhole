#!/bin/bash 
python /home/tkastylevsky/code/out_to_bedtools.py /home/tkastylevsky/trash/Avian_lib_out/galgal6_ensembl_lib.fa.out whole_lib_ensembl whole
python /home/tkastylevsky/code/out_to_bedtools.py /home/tkastylevsky/FASTA_files/repeatmasker/gallus_gallus/gallus_whole_cluster/whole/galgal6_whole.fa.out whole_libwhole whole
bedtools intersect -a /home/tkastylevsky/trash/Avian_lib_out/galgal6_ensembl_lib.fa.out.whole_lib_ensembl.gff -b /home/tkastylevsky/FASTA_files/repeatmasker/gallus_gallus/gallus_whole_cluster/whole/galgal6_whole.fa.out.whole_libwhole.gff -s -wo > /home/tkastylevsky/results/results_bedtools/whole_lib_ensembl_whole_libwhole.txt
bedtools intersect -a /home/tkastylevsky/trash/Avian_lib_out/galgal6_ensembl_lib.fa.out.whole_lib_ensembl.gff -b /home/tkastylevsky/FASTA_files/repeatmasker/gallus_gallus/gallus_whole_cluster/whole/galgal6_whole.fa.out.whole_libwhole.gff -s -wo -f 0.80 -r > /home/tkastylevsky/results/results_bedtools//whole_lib_ensembl_whole_libwhole_80.txt
python /home/tkastylevsky/code/bedtool_analysis.py /home/tkastylevsky/results/results_bedtools/ whole_lib_ensembl whole_libwhole /home/tkastylevsky/trash/Avian_lib_out/galgal6_ensembl_lib.fa.out.whole_lib_ensembl.csv /home/tkastylevsky/FASTA_files/repeatmasker/gallus_gallus/gallus_whole_cluster/whole/galgal6_whole.fa.out.whole_libwhole.csv /home/tkastylevsky/results/bedtools_analysis/
Rscript /home/tkastylevsky/code/bedtools_display.R /home/tkastylevsky/results/bedtools_analysis/ whole_lib_ensembl whole_libwhole
Rscript /home/tkastylevsky/code/bedtools_missing_display.R /home/tkastylevsky/results/bedtools_analysis/ /home/tkastylevsky/trash/Avian_lib_out/galgal6_ensembl_lib.fa.out.whole_lib_ensembl.csv /home/tkastylevsky/FASTA_files/repeatmasker/gallus_gallus/gallus_whole_cluster/whole/galgal6_whole.fa.out.whole_libwhole.csv whole_lib_ensembl whole_libwhole
